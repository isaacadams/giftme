{
  "compilerOptions": {
    "module": "CommonJS",
    "target": "ES3",
    "declaration": true,
    "lib": ["DOM", "ES2022"],
    "jsx": "react",
    "sourceMap": true,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "baseUrl": ".",
    "paths": {
      "#/*": ["./src/*"]
    }
  },
  "exclude": ["node_modules"]
}

