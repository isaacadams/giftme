{
  "name": "giftme",
  "version": "1.0.0",
  "description": "create a wishlist of gifts, share it with your friends and family, and coordinate with them",
  "author": "Isaac Adams",
  "source": "src/index.html",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "format": "prettier --write src/**/*",
    "prestart": "rimraf dist .parcel-cache && npm run format",
    "start": "parcel src/index.html",
    "preprod": "rimraf build",
    "prod": "parcel build src/index.html"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/isaacadams/giftme.git"
  },
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/isaacadams/giftme/issues"
  },
  "homepage": "https://github.com/isaacadams/giftme#readme",
  "alias": {
    "#/*": "./src/$1"
  },
  "dependencies": {
    "grommet": "^2.16.3",
    "grommet-icons": "^4.5.0",
    "react-is": "^17.0.1",
    "rimraf": "^3.0.2",
    "styled-components": "^5.2.1",
    "url-safe-base64": "^1.1.1"
  },
  "devDependencies": {
    "@isaacadams/extensions": "^1.1.1",
    "@parcel/transformer-typescript-tsc": "^2.7.0",
    "@types/node": "^18.11.0",
    "@types/react": "16.14.0",
    "@types/react-dom": "16.8.0",
    "@types/react-router-dom": "^5.1.6",
    "@types/url-safe-base64": "^1.1.0",
    "firebase": "^9.12.1",
    "parcel": "^2.7.0",
    "parcel-resolver-tspaths": "^0.0.9",
    "path-browserify": "^1.0.1",
    "prettier": "^2.2.1",
    "process": "^0.11.10",
    "react": "16.14.0",
    "react-dom": "16.8.0",
    "react-router-dom": "5.2.0",
    "typescript": "^4.1.3"
  }
}

