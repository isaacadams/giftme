{
  "name": "giftme",
  "version": "1.0.0",
  "description": "create a wishlist of gifts, share it with your friends and family, and coordinate with them",
  "author": "Isaac Adams",
  "main": "build/index.html",
  "source": "src/index.html",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "format": "prettier --write src/**/*",
    "prestart": "rimraf dist .parcel-cache && npm run format",
    "start": "parcel src/index.html",
    "preprod": "rimraf build",
    "prod": "parcel build src/index.html"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/isaacadams/giftme.git"
  },
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/isaacadams/giftme/issues"
  },
  "homepage": "https://github.com/isaacadams/giftme#readme",
  "alias": {
    "@config": "./src/config/index.ts",
    "@database": "./src/database/index.ts",
    "@firebase": "./src/firebase/index.ts",
    "@shared": "./src/shared/index.ts",
    "@pages": "./src/pages/index.ts"
  },
  "dependencies": {
    "grommet": "^2.16.2",
    "grommet-icons": "^4.5.0",
    "react-is": "^17.0.1",
    "rimraf": "^3.0.2",
    "styled-components": "^5.2.1",
    "url-safe-base64": "^1.1.1"
  },
  "devDependencies": {
    "@isaacadams/extensions": "^1.1.1",
    "@types/react": "16.14.0",
    "@types/react-dom": "16.8.0",
    "@types/react-router-dom": "^5.1.6",
    "@types/url-safe-base64": "^1.1.0",
    "firebase": "^8.2.1",
    "parcel": "^2.0.0-beta.1",
    "prettier": "^2.2.1",
    "react": "16.14.0",
    "react-dom": "16.8.0",
    "react-router-dom": "5.2.0",
    "typescript": "^4.1.3"
  }
}
